CHIP Memory {
    IN in[16], load, address[15];
    OUT out[16];

    PARTS:
    DMux(in=load, sel=address[14], a=loadram, b=loadothers);
    DMux(in=loadothers, sel=address[13], a=loadscreen, b=loadkeyboard);
    
    RAM16K(in=in, load=loadram, address=address[0..13], out=outram);
    Screen(in=in, load=loadscreen, address=address[0..12], out=outscreen);
    Keyboard(out=outkey);

    Mux4Way16(a=outram, b=outram, c=outscreen, d=outkey, sel[1]=address[14], sel[0]=address[13], out=out);
}